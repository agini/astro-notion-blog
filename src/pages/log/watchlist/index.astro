---
import Layout from "../../layouts/Layout.astro";
import { getPostsByTagAndPage } from "../../../lib/notion";
const posts = await getPostsByTagAndPage("Watchlist");
---

<h1>気になる作品</h1>

<ul>
  {posts.map((post) => (
    <li>
      <a href={`/log/${post.slug}`}>{post.title}</a>
    </li>
  ))}
</ul>
