---
import Layout from "../../layouts/Layout.astro";
import { getPostsByTagAndPage } from "../../lib/notion";
const posts = await getPostsByTagAndPage("experience");
---

<h1>体験log</h1>

<ul>
  {posts.map((post) => (
    <li>
      <a href={`/log/${post.slug}`}>{post.title}</a>
    </li>
  ))}
</ul>
